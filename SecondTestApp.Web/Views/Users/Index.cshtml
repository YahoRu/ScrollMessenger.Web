@model UsersViewModel

@{
    ViewBag.Title = "Users";
    Layout = "_Layout";
}

<div class="container">
    <div class="col-md-12">
        <h2>Users List</h2>
        <ul>
            @foreach (var user in Model.Users)
            {
                <li>@user.Name @user.Email</li>
            }
        </ul>
    </div>
    @await Html.PartialAsync("_SubmitUser", (CreateUserViewModel)null)
</div> 