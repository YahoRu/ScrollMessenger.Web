@model ChatsViewModel

@{
    ViewBag.Title = "Чаты";
    Layout = "_Layout";
}

<div class="container">
    <div class="col-md-12">
        <h2>Список чатов:</h2>
        <ul>
            @foreach (var chat in Model.Chats)
            {
                <li>@chat.ChatName @chat.CreatorId @chat.MultipleChat</li>
            }
        </ul>
    </div>
</div>